
@using WebApplication1.DAL.Entities

@model List<Employee>


<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Age</td>
               
            </tr>

        }
        
    </tbody>
</table>